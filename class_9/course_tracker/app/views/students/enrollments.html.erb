<h1>Enrollments for <%= Student.find(params[:id]).name %></h1>
<table>
<tr><td>Enrollment ID</td><td>Student ID</td><td>Course ID</td></tr>
<% @enrollments.each do |enrollment| %>
  <tr>
    <td><%= enrollment.id %></td>
    <td><%= enrollment.student_id %></td>
    <td><%= enrollment.course_id %></td>
  </tr>
<% end %>
</table>

<%= form_for @enrollment, method: :post do |f| %>
  <%= f.text_field :course_id, :placeholder => "Course ID" %>
<% end %>


<%= collection_select(:student, :course, Course.all, :id, :name, {}, {multiple: true}) %>